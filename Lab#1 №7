#include <iostream>
using namespace std;

//для определения, является ли число простым
bool isPrime(int num) {
	
    if (num <= 1) {
        return false;
    }
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) {
            return false;
        }
    }
    return true;
}

//для удаления простых чисел из массива
int* removePrimes(int* arr, int& size) {
    int newSize = 0;

    
    for (int i = 0; i < size; i++) {
        if (!isPrime(arr[i])) {
            newSize++;
        }
    }

    //новый массив
    int* result = new int[newSize];
    int newIndex = 0;

    // Копируем  в новый массив
    for (int i = 0; i < size; i++) {
        if (!isPrime(arr[i])) {
            result[newIndex++] = arr[i];
        }
    }


    size = newSize;

    return result;
}

int main() {
	setlocale(0,"Russian");
    int size;

    
    cout << "Введите размер массива: ";
    cin >> size;

    
    int* arr = new int[size];
    cout << "Введите элементы массива: ";
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }

    //для удаления простых чисел
    int* result = removePrimes(arr, size);

    
    cout << "Массив без простых чисел: ";
    for (int i = 0; i < size; i++) {
        cout << result[i] << " ";
    }
    cout <<endl;

    return 0;
}
