#include <iostream>
using namespace std;

int main() {
	setlocale(0,"Russian");
    int M, N;

    // Ввод размеров массивов A и B
    cout << "Введите размер массива A: ";
    cin >> M;
    cout << "Введите размер массива B: ";
    cin >> N;

    // Создаем массивы A и B и заполняем их
    int A[M];
    int B[N];

    cout << "Введите элементы массива A:";
    for (int i = 0; i < M; i++) {
        cin >> A[i];
    }

    cout << "Введите элементы массива B: ";
    for (int i = 0; i < N; i++) {
        cin >> B[i];
    }

    // Создаем третий массив для хранения результатов
    int C[M + N]; // Максимальный размер - сумма размеров A и B

    int k = 0; // Индекс для массива C

    // Поиск элементов, не являющихся общими для массивов A и B
    for (int i = 0; i < M; i++) {
        bool isCommon = false;
        for (int j = 0; j < N; j++) {
            if (A[i] == B[j]) {
                isCommon = true;
                break;
            }
        }
        if (!isCommon) {
            C[k++] = A[i];
        }
    }

    for (int i = 0; i < N; i++) {
        bool isCommon = false;
        for (int j = 0; j < M; j++) {
            if (B[i] == A[j]) {
                isCommon = true;
                break;
            }
        }
        if (!isCommon) {
            C[k++] = B[i];
        }
    }

    // Выводим результат
    cout << "Элементы, не являющиеся общими для массивов A и B: ";
    for (int i = 0; i < k; i++) {
        cout << C[i] << " ";
    }
    cout << endl;

    return 0;
}
