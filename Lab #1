#include <iostream>
#include <algorithm>
using namespace std;

int main() {
	setlocale(0,"Russian");
    int M, N;

    
    cout << "Введите размер массива A: ";
    cin >> M;
    cout << "Введите размер массива B: ";
    cin >> N;

    
    int A[M];
    int B[N];

    
    cout << "Введите элементы массива A:" <<endl;
    for (int i = 0; i < M; i++) {
        cin >> A[i];
    }

   
    cout << "Введите элементы массива B:" <<endl;
    for (int i = 0; i < N; i++) {
        cin >> B[i];
    }

    
    sort(A, A + M);
    sort(B, B + N);

   
    int common[M + N]; 

    
    int* C = set_intersection(A, A + M, B, B + N, common);

    
    int common_size = C - common;

    
    cout << "Общие элементы массивов A и B без повторений:" <<endl;
    for (int i = 0; i < common_size; i++) {
       cout << common[i] << " ";
    }
    cout << endl;

    return 0;
}
